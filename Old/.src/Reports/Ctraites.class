' Gambas class file

R As Integer
G As Integer
B As Integer
Coln As Boolean
Inherits PdfWriter

Public Sub _new()
  
  R = Start.R
  G = Start.G
  B = Start.B
  Me.SetFillColor(R, G, B)
  
End

Public Sub _free()
  
End

'---------------------------- Entete des impressions normales ----------------------------------------
Public Sub EnteteN(ent As String)
  
  Me.AddPage
  Me.SetXY(2, 5)
  Me.SetFont("Helvetica", "B", 16)
  Me.SetFillColor(R, G, B)
  Me.cell(206, 8, ent, True, 0, "C", True)
  
End